{
  "name": "monorepo-best-practice",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "workspaces": [
        "apps/*",
        "libs/*"
      ],
      "devDependencies": {
        "typescript": "5.2.2"
      }
    },
    "apps/backend": {
      "name": "@apps/backend"
    },
    "apps/frontend": {},
    "libs/config": {},
    "libs/database": {},
    "libs/idl": {},
    "node_modules/@apps/backend": {
      "resolved": "apps/backend",
      "link": true
    },
    "node_modules/@apps/frontend": {
      "resolved": "apps/frontend",
      "link": true
    },
    "node_modules/@libs/config": {
      "resolved": "libs/config",
      "link": true
    },
    "node_modules/@libs/database": {
      "resolved": "libs/database",
      "link": true
    },
    "node_modules/@libs/idl": {
      "resolved": "libs/idl",
      "link": true
    },
    "node_modules/typescript": {
      "version": "5.2.2",
      "resolved": "https://registry.npmjs.org/typescript/-/typescript-5.2.2.tgz",
      "integrity": "sha512-mI4WrpHsbCIcwT9cF4FZvr80QUeKvsUsUvKDoR+X/7XHQH98xYD8YHZg7ANtz2GtZt/CBq2QJ0thkGJMHfqc1w==",
      "dev": true,
      "bin": {
        "tsc": "bin/tsc",
        "tsserver": "bin/tsserver"
      },
      "engines": {
        "node": ">=14.17"
      }
    }
  }
}
